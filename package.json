{
  "name": "teamplate",
  "version": "16.0.0",
  "private": true,
  "description": "start teamplate",
  "devDependencies": {
    "@babel/preset-env": "^7.4.4",
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "9.5.x",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.5",
    "del": "^4.1.0",
    "gulp": "4.0.x",
    "gulp-babel": "^8.0.0",
    "gulp-csso": "^3.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-iife": "^0.4.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "gulp-svgstore": "^7.0.1",
    "gulp-uglyfly": "^1.4.2",
    "gulp-webp": "^4.0.1",
    "husky": "^2.3.0",
    "imagemin-mozjpeg": "^8.0.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "stylelint": "^9.10.1",
    "stylelint-config-htmlacademy": "0.1.x",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-order": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.css": [
        "prettier --write",
        "stylelint --fix --config ./.stylelintrc-format",
        "git add"
      ]
    }
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp start",
    "server": "gulp server"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.15"
  }
}
